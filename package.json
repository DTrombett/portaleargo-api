{
	"name": "portaleargo-api",
	"version": "1.0.0",
	"description": "A library to interact with the api of ArgoScuolaNext",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup && tsc --emitDeclarationOnly",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"start": "npm run build && node --trace-warnings --enable-source-maps .",
		"test": "npm run test:cli && tsup _test.ts && node dist/_test.js --enable-source-maps",
		"test:cli": "eslint src && tsc --noEmit",
		"postinstall": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/portaleargo-api.git"
	},
	"author": "DTrombett",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DTrombett/portaleargo-api/issues"
	},
	"homepage": "https://github.com/DTrombett/portaleargo-api#readme",
	"engines": {
		"node": ">=20"
	},
	"dependencies": {
		"@tsconfig/node20": "^20.1.2",
		"@tsconfig/recommended": "^1.0.3",
		"@tsconfig/strictest": "^2.0.2",
		"@types/crypto-js": "^4.2.1",
		"@types/node": "^20.9.2",
		"ajv": "^8.12.0",
		"tsup": "^8.0.0",
		"typescript": "^5.2.2",
		"undici": "^5.27.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.54.0",
		"eslint-plugin-node": "^11.1.0",
		"prettier": "^3.1.0"
	}
}
